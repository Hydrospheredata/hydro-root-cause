{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "required": [
    "model",
    "explained_instance"
  ],
  "properties": {
    "explained_instance": {
      "type": "object",
      "required": [
        "uid",
        "timestamp"
      ],
      "properties": {
        "uid": {
          "type": "integer"
        },
        "timestamp": {
          "type": "integer"
        }
      }
    },
    "model": {
      "type": "object",
      "required": [
        "name",
        "version"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "version": {
          "type": "integer"
        }
      }
    }
  }
}